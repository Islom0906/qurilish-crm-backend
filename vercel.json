{
  "version": 2,
  "env": {
    "MONGO_URI": "mongo_uri",
    "PORT": "port",
    "SECRET_JWT":"secret_jwt"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}