{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger",
      "dest": "/swagger.html"
    },
    {
      "src": "/swagger.json",
      "dest": "/swagger.json"
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}